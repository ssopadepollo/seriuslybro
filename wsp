--TP AREAS
--Sky
local work = game.Workspace
local part = Instance.new("Part", work)
part.Name = "SkyPart"
local Skyp = work.SkyPart
Skyp.Size = Vector3.new(5, 145, 5)
Skyp.Anchored = true
Skyp.Position = Vector3.new(-504, 65, -605)
Skyp.Transparency = 0.7
Skyp.CanCollide = false
Skyp.Color = Color3.fromRGB(0, 255, 0)
Skyp.Material = "Neon"

--Touched Area For TP
--Sky
local plrl = game.Players.LocalPlayer
local work = game.Workspace
local Skyp = work.SkyPart

Skyp.Touched:Connect(function()
plrl.Character.HumanoidRootPart.CFrame = CFrame.new(-504, 151, -605)
end)
